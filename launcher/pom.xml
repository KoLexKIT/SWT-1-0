<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>kit.swt1.portunus</groupId>
		<artifactId>portunus</artifactId>
		<version>1.0</version>
	</parent>
	<artifactId>launcher</artifactId>
	<name>Portunus Launcher</name>
	<dependencies>
		<dependency>
			<groupId>org.openjfx</groupId>
			<artifactId>javafx-controls</artifactId>
		</dependency>
		<dependency>
			<groupId>kit.swt1.portunus</groupId>
			<artifactId>core</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>kit.swt1.portunus</groupId>
			<artifactId>controller</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>kit.swt1.portunus</groupId>
			<artifactId>view</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>kit.swt1.portunus</groupId>
			<artifactId>util.io</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>kit.swt1.portunus</groupId>
			<artifactId>util.passwordgenerator</artifactId>
			<version>1.0</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.openjfx</groupId>
				<artifactId>javafx-maven-plugin</artifactId>
				<executions>
					<execution>
						<id>default-cli</id>
						<configuration>
							<mainClass>portunus.launcher.Portunus</mainClass>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>
